<project name="webservices12_servlet_WebServiceRefsTest_server1" basedir="." default="usage">
	<property name="app.name" value="WebServiceRefsTestService1"/>
	<property name="service.names" value="WSHello1Service"/>
  	<property name="sei.names" value="WSHello1"/>
  	<property name="impl.class" value="com.sun.ts.tests.webservices12.servlet.WebServiceRefsTest.server1.WSHello1Servlet"/>
	<property name="wsdl.filename" value="WSHello1Service.wsdl"/>

	<!-- Import common classes properties and specific packaging file -->
	<import file="../../../../../../../../../src/com/sun/ts/tests/jaxws/common/xml/common.xml"/>

  	<property name="wsdl.file" value="${src.dir}/${pkg.dir}/${wsdl.filename}"/>

	<property name="common.classes" value="${jaxws.common.classes}"/>

	<target name="-postcompile" >
	    <antcall target="do_java2wsdl_generate_server"/>
  	</target>

  	<target name="package" depends="j2w_webservice_import_package"/>

  	<target name="-preclean">
    	    <remove_wsdl_xsd/>
	</target>

</project>
