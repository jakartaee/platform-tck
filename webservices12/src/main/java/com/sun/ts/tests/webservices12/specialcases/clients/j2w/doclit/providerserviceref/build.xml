<project name="webservices12_specialcases_clients_j2w_doclit_providerserviceref" basedir="." default="usage">
  <!-- Import common classes properties and specific packaging file -->
  <import file="../../../../../../../../../../../src/com/sun/ts/tests/jaxws/common/xml/common.xml"/>
  <property file="${bin.dir}/webservices-url-props.dat"/>

  <property name="app.name" value="WSJ2WDLSCProviderWSRef"/>
  <property name="app.name.endpoint" value="WSW2JDLSCProvider"/>
  <property name="wsdl.file" value="http://${webServerHost}:${webServerPort}${specialcases.providerserviceref.wsdlloc}"/>
  <property name="wsdl.location" value="${wsdl.file}"/>

  <property name="client.class" value="com.sun.ts.tests.webservices12.specialcases.clients.j2w.doclit.providerserviceref.Client"/>
  <property name="common.classes" value="${jaxws.common.classes}"/>
  <property name="other.classes" value="endpoint/jaxws/*.class"/>
  <property name="always.generate" value="true"/>



  <import file="../../../special.build.xml"/>

  <target name="-precompile" >
     <if>
        <istrue value="${_build.webservices12.specialcases.clients}"/>
     <then>
        <echo message="wsdl.file=${wsdl.file}"/>
        <antcall  target="do_wsdl2java_generate_client"/>
     </then>
     </if>
  </target>
  <target name="compile" >
     <if>
        <istrue value="${_build.webservices12.specialcases.clients}"/>
     <then>
        <ts.javac/>
     </then>
     </if>
  </target>
  <target name="package" >
     <if>
        <istrue value="${_build.webservices12.specialcases.clients}"/>
     <then>
        <antcall target="client_import_package"/>
     </then>
     </if>
  </target>


  <target name="-preclean">
    <remove_wsdl_xsd/>
  </target>


</project>
