<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
  <display-name>servlet_vehicle</display-name>
  <servlet>
   <servlet-name>Servlet_VehicleLogicalName</servlet-name>
   <servlet-class>com.sun.ts.tests.common.vehicle.servlet.ServletVehicle</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>Servlet_VehicleLogicalName</servlet-name>
    <url-pattern>/servlet_vehicle</url-pattern>
  </servlet-mapping>
  <session-config>
   <session-timeout>54</session-timeout>
  </session-config>        
  <jms-connection-factory>
      <description>Define ConnectionFactory ServletMyTestConnectionFactory</description>
      <name>java:global/ServletMyTestConnectionFactory</name>
      <interface-name>javax.jms.ConnectionFactory</interface-name>
      <user>@user@</user>
      <password>@password@</password>
  </jms-connection-factory>
  <jms-connection-factory>
      <description>Define QueueConnectionFactory ServletMyTestQueueConnectionFactory</description>
      <name>java:app/ServletMyTestQueueConnectionFactory</name>
      <interface-name>javax.jms.QueueConnectionFactory</interface-name>
      <user>@user@</user>
      <password>@password@</password>
  </jms-connection-factory>
  <jms-connection-factory>
      <description>Define TopicConnectionFactory ServletMyTestTopicConnectionFactory</description>
      <name>java:module/ServletMyTestTopicConnectionFactory</name>
      <interface-name>javax.jms.TopicConnectionFactory</interface-name>
      <user>@user@</user>
      <password>@password@</password>
  </jms-connection-factory>
  <jms-connection-factory>
      <description>Define Durbale TopicConnectionFactory ServletMyTestDurableTopicConnectionFactory</description>
      <name>java:comp/env/jms/ServletMyTestDurableTopicConnectionFactory</name>
      <interface-name>javax.jms.TopicConnectionFactory</interface-name>
      <user>@user@</user>
      <password>@password@</password>
      <client-id>MyClientID</client-id>
      <property>
	<name>Property1</name>
	<value>10</value>
      </property>
      <property>
	<name>Property2</name>
	<value>20</value>
      </property>
      <transactional>false</transactional>
      <max-pool-size>30</max-pool-size>
      <min-pool-size>20</min-pool-size>
  </jms-connection-factory>
  <jms-destination>
      <description>Define Queue ServletMyTestQueue</description>
      <name>java:global/env/ServletMyTestQueue</name>
      <interface-name>javax.jms.Queue</interface-name>
      <destination-name>ServletMyTestQueue</destination-name>
  </jms-destination>
  <jms-destination>
      <description>Define Topic ServletMyTestTopic</description>
      <name>java:app/env/ServletMyTestTopic</name>
      <interface-name>javax.jms.Topic</interface-name>
      <destination-name>ServletMyTestTopic</destination-name>
  </jms-destination>
</web-app>
