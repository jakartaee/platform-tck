<sun-ejb-jar>
  <enterprise-beans>
    <unique-id>0</unique-id>
    <ejb>
      <ejb-name>HttpTwin3Test_helloEjb</ejb-name>
      <jndi-name>HttpTwin3Test_helloEjb</jndi-name>
