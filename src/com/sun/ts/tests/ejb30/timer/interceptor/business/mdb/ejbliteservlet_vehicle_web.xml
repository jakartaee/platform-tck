<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>EJBLiteServletVehicle</servlet-name>
    <servlet-class>com.sun.ts.tests.ejb30.timer.interceptor.business.mdb.EJBLiteServletVehicle</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EJBLiteServletVehicle</servlet-name>
    <url-pattern>/ejbliteservlet_vehicle.jsp</url-pattern>
  </servlet-mapping>
  
  <message-destination-ref>
    <message-destination-ref-name>sendQueue</message-destination-ref-name>
    <message-destination-type>jakarta.jms.Queue</message-destination-type>
    <message-destination-usage>Produces</message-destination-usage>
    <message-destination-link>MsgBeanInQueue</message-destination-link>
  </message-destination-ref>
  
</web-app>
