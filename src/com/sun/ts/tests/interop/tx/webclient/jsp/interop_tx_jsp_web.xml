<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>interopTxJspWebApp</display-name>
  <servlet>
    <servlet-name>MyJspBean2EjbLogicalName</servlet-name>
    <jsp-file>/jspbean2ejb.jsp</jsp-file>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <session-config>
    <session-timeout>54</session-timeout>
  </session-config>
  <env-entry>
    <env-entry-name>table1</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>ctstable1</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>table2</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>ctstable2</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>size</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>5</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>fromKey1</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>2</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>fromKey2</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>2</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>toKey2</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>3</env-entry-value>
  </env-entry>
  <ejb-ref>
    <ejb-ref-name>ejb/TxBeanManaged</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.tx.txbean.TxBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.tx.txbean.TxBean</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/TxNotSupported</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.tx.txbean.TxBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.tx.txbean.TxBean</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/TxSupports</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.tx.txbean.TxBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.tx.txbean.TxBean</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/TxRequired</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.tx.txbean.TxBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.tx.txbean.TxBean</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/TxRequiresNew</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.tx.txbean.TxBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.tx.txbean.TxBean</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/TxMandatory</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.tx.txbean.TxBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.tx.txbean.TxBean</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/TxNever</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.tx.txbean.TxBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.tx.txbean.TxBean</remote>
  </ejb-ref>
</web-app>
