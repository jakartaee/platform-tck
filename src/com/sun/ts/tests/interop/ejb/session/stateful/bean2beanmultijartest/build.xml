<!-- TestBean1.java and TestBean1Home.java need to be included in j2ee2.jar as TestBean2 has reference to it -->
<project name="interop_ejb_session_stateful_bean2beanmultijartest" basedir="." default="usage">
<import file="../../../../../../../../../../bin/xml/ts.import.xml"/> 


  <target name="package">

      <ts.ejbjar descriptor="interop_ejb_Ssf_b2bmultib2_j2ee2_ejb.xml" 
                 archivename="interop_ejb_Ssf_b2bmultib2_j2ee2"
                 excludedfiles="com/sun/ts/tests/interop/ejb/session/stateful/bean2beanmultijartest/bean1/TestBean1EJB.class"/>

      <ts.ear archivename="interop_ejb_Ssf_b2bmultib2_j2ee2" includedefaultfiles="false">
        <fileset dir="${dist.dir}/${pkg.dir}" includes="interop_ejb_Ssf_b2bmultib2_j2ee2_ejb.jar"/>
      </ts.ear>

      <ts.ejbjar descriptor="interop_ejb_Ssf_b2bmultib1_ejb.xml"
                 archivename="interop_ejb_Ssf_b2bmultib1"
                 excludedfiles="com/sun/ts/tests/interop/ejb/session/stateful/bean2beanmultijartest/bean2/TestBean2EJB.class"/>

      <ts.clientjar descriptor="interop_ejb_Ssf_b2bmultib1_client.xml"
                    archivename="interop_ejb_Ssf_b2bmultib1"
                    mainclass="com.sun.ts.tests.interop.ejb.session.stateful.bean2beanmultijartest.Client"
                    excludedfiles="com/sun/ts/tests/interop/ejb/session/stateful/bean2beanmultijartest/bean1/TestBean1EJB.class,
                                   com/sun/ts/tests/interop/ejb/session/stateful/bean2beanmultijartest/bean2/TestBean2.class,
                                   com/sun/ts/tests/interop/ejb/session/stateful/bean2beanmultijartest/bean2/TestBean2EJB.class,
                                   com/sun/ts/tests/interop/ejb/session/stateful/bean2beanmultijartest/bean2/TestBean2Home.class"/>

      <ts.ear archivename="interop_ejb_Ssf_b2bmultib1" includedefaultfiles="false">
        <fileset dir="${dist.dir}/${pkg.dir}"
                 includes="interop_ejb_Ssf_b2bmultib1_ejb.jar, interop_ejb_Ssf_b2bmultib1_client.jar"/>
      </ts.ear>

  </target>

</project>
