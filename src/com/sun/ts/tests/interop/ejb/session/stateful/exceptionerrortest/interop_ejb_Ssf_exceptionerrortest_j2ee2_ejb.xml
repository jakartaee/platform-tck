<ejb-jar version="2.1" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
  <display-name>Ejb1</display-name>
  <enterprise-beans>
    <session>
      <ejb-name>interop_ejb_session_stateful_exceptionerrortest_TestBean</ejb-name>
      <home>com.sun.ts.tests.interop.ejb.session.stateful.exceptionerrortest.TestBeanHome</home>
      <remote>com.sun.ts.tests.interop.ejb.session.stateful.exceptionerrortest.TestBean</remote>
      <ejb-class>com.sun.ts.tests.interop.ejb.session.stateful.exceptionerrortest.TestBeanEJB</ejb-class>
      <session-type>Stateful</session-type>
      <transaction-type>Container</transaction-type>
      <security-identity>
        <use-caller-identity/>
      </security-identity>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>interop_ejb_session_stateful_exceptionerrortest_TestBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>throwEJBException</method-name>
      </method>
      <trans-attribute>NotSupported</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>interop_ejb_session_stateful_exceptionerrortest_TestBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>throwError</method-name>
      </method>
      <trans-attribute>NotSupported</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>interop_ejb_session_stateful_exceptionerrortest_TestBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>throwMyApplicationException</method-name>
      </method>
      <trans-attribute>NotSupported</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>interop_ejb_session_stateful_exceptionerrortest_TestBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>ping</method-name>
      </method>
      <trans-attribute>NotSupported</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
