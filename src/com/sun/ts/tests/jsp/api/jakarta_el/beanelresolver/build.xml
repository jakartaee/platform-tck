<project name="jsp_beanelresolver" basedir="." default="usage" >

    <import file="../../../../../../../../../src/com/sun/ts/tests/jsp/common/xml/common.xml"/>
    <property name="app.name"  value="jsp_beanelresolver" />
    <property name="pkg.name"   value="jakarta_el/beanelresolver" />
    <property name="contentroot.dir"  value="${ts.home}/src/web/jsp/api/${pkg.name}" />
    
    <target name="package">
        <tscontent.war descriptor="${app.name}_web.xml" archivename="${app.name}"> 
            <zipfileset dir="${class.dir}"
                includes="${jspcommon.dir}/util/JspTestUtil.class,
                          ${elcommon.dir}/api/resolver/*"
                prefix="${class.prefix}"/>
        </tscontent.war>
    </target>
</project>

