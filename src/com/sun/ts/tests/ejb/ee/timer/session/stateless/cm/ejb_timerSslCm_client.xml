<application-client version="1.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/application-client_1_4.xsd">
  <description>CTS Stateless Timer Container Managed tests</description>
  <display-name>ejb_timerSslCm_client</display-name>
  <ejb-ref>
    <ejb-ref-name>ejb/TestBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.timer.session.stateless.cm.TestBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.timer.session.stateless.cm.TestBean</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ejb/ProxyBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>com.sun.ts.tests.ejb.ee.timer.session.stateless.cm.ProxyBeanHome</home>
    <remote>com.sun.ts.tests.ejb.ee.timer.session.stateless.cm.ProxyBean</remote>
  </ejb-ref>
  <resource-ref>
    <description>JMS QueueConnectionFactory</description>
    <res-ref-name>jms/MyQueueConnectionFactory</res-ref-name>
    <res-type>jakarta.jms.QueueConnectionFactory</res-type>
    <res-auth>Container</res-auth>
    <res-sharing-scope>Shareable</res-sharing-scope>
  </resource-ref>
  <resource-env-ref>
    <resource-env-ref-name>jms/MyQueue</resource-env-ref-name>
    <resource-env-ref-type>jakarta.jms.Queue</resource-env-ref-type>
  </resource-env-ref>
</application-client>
