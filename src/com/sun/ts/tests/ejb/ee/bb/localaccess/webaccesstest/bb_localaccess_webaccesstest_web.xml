  </ejb-local-ref>
  <ejb-local-ref>
    <ejb-ref-name>ejb/BEJB</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local-home>com.sun.ts.tests.ejb.ee.bb.localaccess.webaccesstest.BLocalHome</local-home>
    <local>com.sun.ts.tests.ejb.ee.bb.localaccess.webaccesstest.BLocal</local>
