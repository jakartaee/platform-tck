<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>bb_localaccess_webaccesstest</display-name>
  <servlet>
    <servlet-name>MyJsp2AEjbLogicalName</servlet-name>
    <jsp-file>/jsp2aejb.jsp</jsp-file>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>MyJsp2BEjbLogicalName</servlet-name>
    <jsp-file>/jsp2bejb.jsp</jsp-file>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>MyJsp2CEjbLogicalName</servlet-name>
    <jsp-file>/jsp2cejb.jsp</jsp-file>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>MyJsp2DEjbLogicalName</servlet-name>
    <jsp-file>/jsp2dejb.jsp</jsp-file>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <session-config>
    <session-timeout>54</session-timeout>
  </session-config>
  <ejb-local-ref>
    <ejb-ref-name>ejb/AEJB</ejb-ref-name>
    <ejb-ref-type>Entity</ejb-ref-type>
    <local-home>com.sun.ts.tests.ejb.ee.bb.localaccess.webaccesstest.ALocalHome</local-home>
    <local>com.sun.ts.tests.ejb.ee.bb.localaccess.webaccesstest.ALocal</local>
