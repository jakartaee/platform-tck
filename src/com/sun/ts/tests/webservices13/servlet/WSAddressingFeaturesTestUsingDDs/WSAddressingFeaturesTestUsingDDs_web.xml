<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name>WSAddressingFeaturesTestUsingDDs</display-name>
  <servlet>
    <servlet-name>EchoImpl</servlet-name>
    <servlet-class>com.sun.ts.tests.webservices13.servlet.WSAddressingFeaturesTestUsingDDs.EchoImpl</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Echo2Impl</servlet-name>
    <servlet-class>com.sun.ts.tests.webservices13.servlet.WSAddressingFeaturesTestUsingDDs.Echo2Impl</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Echo3Impl</servlet-name>
    <servlet-class>com.sun.ts.tests.webservices13.servlet.WSAddressingFeaturesTestUsingDDs.Echo3Impl</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Echo4Impl</servlet-name>
    <servlet-class>com.sun.ts.tests.webservices13.servlet.WSAddressingFeaturesTestUsingDDs.Echo4Impl</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>EchoImpl</servlet-name>
    <url-pattern>/jaxws/Echo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Echo2Impl</servlet-name>
    <url-pattern>/jaxws/Echo2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Echo3Impl</servlet-name>
    <url-pattern>/jaxws/Echo3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Echo4Impl</servlet-name>
    <url-pattern>/jaxws/Echo4</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>54</session-timeout>
  </session-config>
</web-app>

