<project name="jws_oneway_server" basedir="." default="usage">
  <property name="app.name" value="WSOnewayWebServiceApp"/>
  <property name="service.names" value="OnewayWebServiceService"/>
  <property name="sei.names" value="OnewayWebService"/>  
  <property name="port.name" value="OnewayWebServicePort"/>
  <property name="wsdl.filename" value="OnewayWebServiceService.wsdl"/>
  <property name="impl.class" value="com.sun.ts.tests.jws.oneway.server.OnewayWebService"/>

  <!-- Import common classes properties and specific packaging file -->
  <import file="../../../../../../../../src/com/sun/ts/tests/jws/common/xml/common.xml"/>

  <property name="wsdl.file" value="${src.dir}/${pkg.dir}/${wsdl.filename}"/>

  <property name="impl.classes" value="${pkg.dir}/OnewayWebService.class"/>
  <property name="common.classes" value="${jws.common.classes}"/>

  <target name="-postcompile" >
    <antcall  target="do_java2wsdl_generate_server"/>
  </target>

  <target name="package" depends="j2w_webservice_import_package"/>

 <target name="-preclean">
    <remove_wsdl_xsd/>
  </target>


</project>
