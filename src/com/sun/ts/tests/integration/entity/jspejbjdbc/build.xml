<project name="integration_entity_jspejbjdbc" basedir="." default="usage">
<import file="../../../../../../../../bin/xml/ts.import.xml"/> 

  <target name="package">
      <ts.ejbjar descriptor="int_E_jspejbjdbc_ejb.xml" archivename="int_E_jspejbjdbc"
                 excludedfiles="com/sun/ts/tests/integration/entity/jspejbjdbc/AccessJSPBean.class">
        <fileset dir="${class.dir}" includes="com/sun/ts/tests/integration/util/Account.class, com/sun/ts/tests/integration/util/DBSupport.class"/>
      </ts.ejbjar>
      <ts.war descriptor="int_E_jspejbjdbc_web.xml" archivename="int_E_jspejbjdbc"
              excludedfiles="com/sun/ts/tests/integration/entity/jspejbjdbc/AccountBean.class">
        <zipfileset dir="${class.dir}" includes="com/sun/ts/tests/integration/entity/jspejbjdbc/AccessJSPBean.class"
                    prefix="WEB-INF/classes"/>
        <zipfileset dir="./contentRoot" includes="*"/>
      </ts.war>

      <ts.ear archivename="int_E_jspejbjdbc"/>
  </target>

</project>
