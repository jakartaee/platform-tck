<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm
    http://xmlns.jcp.org/xml/ns/persistence/orm_2_1.xsd"
    version="2.1">
    <entity class="com.sun.ts.tests.jpa.core.override.joincolumn.TheatreCompany1">
        <attributes>
            <one-to-many name="locations" mapped-by="company">
                <cascade>
                    <cascade-persist/>
                </cascade>
            </one-to-many>
        </attributes>
    </entity>
    <entity class="com.sun.ts.tests.jpa.core.override.joincolumn.TheatreLocation1">
        <attributes>
            <many-to-one name="company">
                <join-column name="TCOMP_ID" referenced-column-name="ID"/>
            </many-to-one>
        </attributes>
    </entity>
    <entity class="com.sun.ts.tests.jpa.core.override.joincolumn.Course">
        <attributes>
            <many-to-many name="students" mapped-by="courses">
                <cascade>
                    <cascade-all/>
                </cascade>
            </many-to-many>
        </attributes>
    </entity>
    <entity class="com.sun.ts.tests.jpa.core.override.joincolumn.Student">
        <attributes>
            <many-to-many name="courses">
                <join-table name="STUDENT_2_COURSE_2">
                    <join-column name="STUDENTS_ID"/>
                    <inverse-join-column name="COURSES_ID"/>
                </join-table>
            </many-to-many>
        </attributes>
    </entity>
    <entity class="com.sun.ts.tests.jpa.core.override.joincolumn.Cubicle">
    </entity>
    <entity class="com.sun.ts.tests.jpa.core.override.joincolumn.Hardware">
        <attributes>
            <many-to-one name="cubicle">
                <join-column name="CUBE_ID" referenced-column-name="ID"/>
                <join-column name="CUBE_LOCATION" referenced-column-name="LOCATION"/>
            </many-to-one>
        </attributes>
    </entity>
    <entity class="com.sun.ts.tests.jpa.core.override.joincolumn.Customer1">
        <attributes>
            <one-to-many name="orders">
                <join-table name="CUST_ORDER">
                    <join-column name="CUSTID"/>
                    <inverse-join-column name="ORDERID"/>
                </join-table>
                <cascade>
                    <cascade-all/>
                </cascade>
            </one-to-many>
        </attributes>
    </entity>
    <entity class="com.sun.ts.tests.jpa.core.override.joincolumn.RetailOrder1">
    </entity>
</entity-mappings>


