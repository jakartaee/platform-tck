<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.3" xmlns:wsdl="http://mtomtestservice.org/wsdl" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_3.xsd">
  <webservice-description>
    <webservice-description-name>MTOMTestService</webservice-description-name>
    <wsdl-file>WEB-INF/wsdl/MTOMTestService.wsdl</wsdl-file>
    <port-component>
      <port-component-name>MTOMTestImpl</port-component-name>
      <wsdl-port>wsdl:MTOMTestPort</wsdl-port>
      <enable-mtom>true</enable-mtom>
      <protocol-binding>##SOAP11_HTTP</protocol-binding>
      <service-endpoint-interface>com.sun.ts.tests.webservices12.servlet.WSMTOMSBFullDDsTest.MTOMTest</service-endpoint-interface>
      <service-impl-bean>
        <servlet-link>MTOMTestImpl</servlet-link>
      </service-impl-bean>
    </port-component>
    <port-component>
      <port-component-name>MTOMTestTwoImpl</port-component-name>
      <wsdl-port>wsdl:MTOMTestTwoPort</wsdl-port>
      <enable-mtom>true</enable-mtom>
      <protocol-binding>##SOAP11_HTTP</protocol-binding>
      <service-endpoint-interface>com.sun.ts.tests.webservices12.servlet.WSMTOMSBFullDDsTest.MTOMTestTwo</service-endpoint-interface>
      <service-impl-bean>
        <servlet-link>MTOMTestTwoImpl</servlet-link>
      </service-impl-bean>
    </port-component>
  </webservice-description> 
</webservices>
