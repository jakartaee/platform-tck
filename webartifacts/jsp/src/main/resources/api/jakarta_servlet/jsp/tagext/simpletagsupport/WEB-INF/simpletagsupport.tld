        <body-content>scriptless</body-content> 
    </tag>
    <tag>
        <name>cskippage</name>
        <tag-class>com.sun.ts.tests.jsp.api.jakarta_servlet.jsp.tagext.simpletagsupport.ClassicSkipPage</tag-class>
        <body-content>JSP</body-content> 
    </tag>
    <tag>
        <name>sskippage</name>
        <tag-class>com.sun.ts.tests.jsp.api.jakarta_servlet.jsp.tagext.simpletagsupport.SimpleSkipPage</tag-class>
        <body-content>scriptless</body-content>
    </tag>
    <tag>
        <name>simpledefault</name>
        <tag-class>com.sun.ts.tests.jsp.api.jakarta_servlet.jsp.tagext.simpletagsupport.DefaultSimpleTag</tag-class>
        <body-content>scriptless</body-content>
    </tag>
    <tag>
