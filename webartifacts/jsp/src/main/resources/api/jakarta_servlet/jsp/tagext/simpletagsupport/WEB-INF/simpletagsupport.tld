        <tag-class>com.sun.ts.tests.jsp.api.jakarta_servlet.jsp.tagext.simpletagsupport.SimpleSyncTag</tag-class>
        <body-content>scriptless</body-content>
        <variable>
            <name-given>begin</name-given>
            <variable-class>java.lang.Integer</variable-class>
            <scope>AT_BEGIN</scope>
        </variable>
        <variable>
            <name-given>end</name-given>
            <variable-class>java.lang.Integer</variable-class>
            <scope>AT_END</scope>
        </variable>
    </tag>
</taglib>
