        </attribute>
        <attribute>
            <name>string</name>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <name>stype</name>
        <tag-class>com.sun.ts.tests.jsp.spec.core_syntax.actions.attribute.SimpleTypeTag</tag-class>
        <body-content>scriptless</body-content>
