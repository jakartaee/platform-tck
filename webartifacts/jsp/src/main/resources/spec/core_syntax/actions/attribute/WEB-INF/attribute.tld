        </attribute>
        <attribute>
            <name>integer</name>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>long</name>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Long</type>
        </attribute>
        <attribute>
            <name>float</name>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Float</type>
        </attribute>
        <attribute>
            <name>double</name>
