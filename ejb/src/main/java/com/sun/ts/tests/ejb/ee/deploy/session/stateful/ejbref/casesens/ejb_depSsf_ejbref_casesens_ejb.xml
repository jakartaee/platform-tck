        <remote>com.sun.ts.tests.ejb.ee.deploy.session.stateful.ejbref.casesens.CaseBean</remote>
        <ejb-link>CaseBean2</ejb-link>
      </ejb-ref>
      <ejb-ref>
        <ejb-ref-name>ejb/Philosopher</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <home>com.sun.ts.tests.ejb.ee.deploy.session.stateful.ejbref.casesens.CaseBeanHome</home>
