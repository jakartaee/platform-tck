<project name="ejb_ee_bb_localaccess_webaccesstest" basedir="." default="usage">
<import file="../../../../../../../../../../bin/xml/ts.import.xml"/> 

  <target name="package">

      <ts.ejbjar descriptor="bb_localaccess_webaccesstest_ejb.xml"
                  archivename="bb_localaccess_webaccesstest">
        <fileset dir="${class.dir}"
                 includes="com/sun/ts/tests/ejb/ee/bb/entity/util/DBSupport.class"/>
      </ts.ejbjar>

      <ts.war descriptor="bb_localaccess_webaccesstest_web.xml"
              archivename="bb_localaccess_webaccesstest"
              excludedfiles="com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/AEJB.class,
                             com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/BEJB.class,
                             com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/CEJB.class,
                             com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/DEJB.class">
        <fileset dir="contentRoot" includes="*"/>
        <zipfileset dir="${class.dir}"
                    includes="com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/ALocalHome.class,
                              com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/ALocal.class,
                              com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/BLocalHome.class,
                              com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/BLocal.class,
                              com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/CLocalHome.class,
                              com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/CLocal.class,
                              com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/DLocalHome.class,
                              com/sun/ts/tests/ejb/ee/bb/localaccess/webaccesstest/DLocal.class"
                    prefix="WEB-INF/classes"/>
      </ts.war>

      <ts.ear archivename="bb_localaccess_webaccesstest"/>

  </target>

</project>
