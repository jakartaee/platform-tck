<project name="jaxws_mapping_w2jmapping_rpc_literal_noncustomization" basedir="." default="usage">
     <property name="app.name" value="WSMAPW2JRLNonCustomization"/>
	<property name="wsdl.filename" value="W2JRLNoncustomization.wsdl"/>

	<!-- Import common classes properties and specific packaging file -->
	<import file="../../../../../../../../../../../src/com/sun/ts/tests/jaxws/common/xml/common.xml"/>

  <property name="wsdl.file" value="${src.dir}/${pkg.dir}/${wsdl.filename}"/>

	<property name="client.class" value="com.sun.ts.tests.jaxws.mapping.w2jmapping.rpc.literal.noncustomization.Client"/>
	<property name="common.classes" value="${jaxws.common.classes}"/>

	<target name="-precompile" >
    		<antcall  target="do_wsdl2java_generate_client">
	        <param name="customization.binding.files" value=""/>
	        <param name="excluded.customization.binding.files" value="**/*"/>
    		</antcall>
     </target>

     <target name="package" depends="client_import_package"/>

</project>
