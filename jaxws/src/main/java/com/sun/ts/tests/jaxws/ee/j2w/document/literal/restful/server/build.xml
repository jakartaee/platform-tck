<project name="jaxws_ee_j2w_document_literal_restful_server" basedir="." default="usage">
  <property name="app.name" value="WSJ2WDLRESTFUL"/>
  <property name="impl.class" value="com.sun.ts.tests.jaxws.ee.j2w.document.literal.restful.server.TokensImpl"/>

  <!-- Import common classes properties and specific packaging file -->
  <import file="../../../../../../../../../../../../src/com/sun/ts/tests/jaxws/common/xml/common.xml"/>

  <property name="impl.classes" value="${pkg.dir}/TokensImpl.class"/>
  <property name="common.classes" value="${jaxws.common.classes}"/>


  <target name="-postcompile" >
    <antcall  target="do_java2wsdl_generate_server">
       <param name="genwsdl" value="false"/>
    </antcall>
  </target>

  <target name="package" depends="j2w_webservice_import_package"/>

  <target name="-preclean">
    <remove_wsdl_xsd/>
  </target>

</project>
