                <port-name-pattern xmlns:ns2="http://foobar.org/">ns2:AddNumbersPort</port-name-pattern>
                <handler>
                    <handler-name>ClientSOAPHandler</handler-name>
                    <handler-class>com.sun.ts.tests.jaxws.wsa.j2w.document.literal.action.ClientSOAPHandler</handler-class>
                </handler>
            </handler-chain>
        </handler-chains>
    </bindings>

</bindings>
