<sun-ejb-jar>
  <security-role-mapping>
    <role-name>Administrator</role-name>
    <principal-name>j2ee</principal-name>
  </security-role-mapping>

<enterprise-beans>
<unique-id>0</unique-id>
<ejb>
<ejb-name>SessionContextAllowedBean</ejb-name>
<jndi-name>ejb3_bb_stateless_bm_allowed_SessionContextAllowedBean</jndi-name>
</ejb>

<ejb>
<ejb-name>AllowedBean</ejb-name>
<jndi-name>ejb3_bb_stateless_bm_allowed_AllowedBean</jndi-name>
</ejb>

<ejb>
<ejb-name>CallbackAllowedBean</ejb-name>
<jndi-name>ejb3_bb_stateless_bm_allowed_CallbackAllowedBean</jndi-name>
</ejb>

<ejb>
<ejb-name>InjectionAllowedBean</ejb-name>
<jndi-name>ejb3_bb_stateless_bm_allowed_InjectionAllowedBean</jndi-name>
</ejb>
</enterprise-beans>
</sun-ejb-jar>
