<ejb-jar xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" metadata-complete="false" version="4.0" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/ejb-jar_4_0.xsd">
    <enterprise-beans>
        <session>
            <ejb-name>AroundInvokeBean</ejb-name>
            <ejb-class>com.sun.ts.tests.ejb30.bb.session.stateless.interceptor.listener.override.AroundInvokeBean</ejb-class>
            <around-invoke>
                <method-name>intercept</method-name>
            </around-invoke>
        </session>
    </enterprise-beans>

    <interceptors>
        <interceptor>
            <interceptor-class>com.sun.ts.tests.ejb30.common.interceptor.Interceptor2</interceptor-class>
            <around-invoke>
                <method-name>intercept2</method-name>
            </around-invoke>
        </interceptor>

        <interceptor>
            <interceptor-class>com.sun.ts.tests.ejb30.common.interceptor.Interceptor1</interceptor-class>
            <around-invoke>
                <method-name>intercept1</method-name>
            </around-invoke>
        </interceptor>
    </interceptors>

    <assembly-descriptor>
        <interceptor-binding>
            <ejb-name>AroundInvokeBean</ejb-name>
            <interceptor-order>
                <interceptor-class>com.sun.ts.tests.ejb30.common.interceptor.Interceptor1</interceptor-class>
                <interceptor-class>com.sun.ts.tests.ejb30.common.interceptor.Interceptor2</interceptor-class>
            </interceptor-order>
        </interceptor-binding>
    </assembly-descriptor>
</ejb-jar>
