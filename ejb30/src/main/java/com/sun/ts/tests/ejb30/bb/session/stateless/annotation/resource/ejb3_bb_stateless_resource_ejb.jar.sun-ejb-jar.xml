                <default-resource-principal>
                    <name>j2ee</name>
                    <password>j2ee</password>
                </default-resource-principal>
            </resource-ref>
            
            <resource-env-ref>
                <resource-env-ref-name>dog</resource-env-ref-name>
                <jndi-name>custom/dog</jndi-name>
            </resource-env-ref>
            
            <resource-env-ref>
                <resource-env-ref-name>dog2</resource-env-ref-name>
                <jndi-name>custom/dog</jndi-name>
            </resource-env-ref>

           <!-- This mapping is not needed.  It should be resolved automatically 
            <resource-env-ref>
