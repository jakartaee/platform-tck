            </resource-ref>
            <resource-ref>
                <res-ref-name>url</res-ref-name>
                <jndi-name>http://webServerHost:webServerPort</jndi-name>
            </resource-ref>
            <resource-ref>
                <res-ref-name>queueConnectionFactory</res-ref-name>
                <jndi-name>jms/QueueConnectionFactory</jndi-name>
