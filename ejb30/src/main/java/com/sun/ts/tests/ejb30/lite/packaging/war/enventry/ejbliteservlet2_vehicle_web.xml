<web-app version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd">
  <filter>
    <filter-name>EJBLiteServlet2Filter</filter-name>
    <filter-class>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.EJBLiteServlet2Filter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EJBLiteServlet2Filter</filter-name>
    <servlet-name>ejbliteservlet2_vehicle.jsp</servlet-name>
  </filter-mapping>
  <servlet>
    <servlet-name>ejbliteservlet2_vehicle.jsp</servlet-name>
    <jsp-file>/ejbliteservlet2_vehicle.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ejbliteservlet2_vehicle.jsp</servlet-name>
    <url-pattern>/ejbliteservlet2_vehicle.jsp</url-pattern>
  </servlet-mapping>

  <env-entry>
    <description>spaces should be preserved</description>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myString</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value> web.xml ${java.home} </env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myBooleanTrue</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value>truE</env-entry-value>  <!-- true -->
  </env-entry>
  
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myBooleanTrue2</env-entry-name>
    <injection-target>
      <injection-target-class>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client</injection-target-class>
      <injection-target-name>myBooleanTrue2</injection-target-name>
    </injection-target>
    <lookup-name>
      java:comp/env/com.sun.ts.tests.ejb30.lite.packaging.war.enventry.OneBean/myBooleanTrue
    </lookup-name>
  </env-entry>  
  
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myBooleanFalse</env-entry-name>
    <env-entry-type>java.lang.Boolean</env-entry-type>
    <env-entry-value>yes</env-entry-value>  <!-- false -->
  </env-entry>
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myByte</env-entry-name>
    <env-entry-type>java.lang.Byte</env-entry-type>
    <env-entry-value>8</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myShort</env-entry-name>
    <env-entry-type>java.lang.Short</env-entry-type>
    <env-entry-value>8</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myInt</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>8</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myLong</env-entry-name>
    <env-entry-type>java.lang.Long</env-entry-type>
    <env-entry-value>8</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myFloat</env-entry-name>
    <env-entry-type>java.lang.Float</env-entry-type>
    <env-entry-value>8</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>com.sun.ts.tests.ejb30.lite.packaging.war.enventry.Client/myDouble</env-entry-name>
    <env-entry-type>java.lang.Double</env-entry-type>
    <env-entry-value>8</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>myChar</env-entry-name>
    <env-entry-type>java.lang.Character</env-entry-type>
    <env-entry-value> </env-entry-value>
  </env-entry>
</web-app>
