<project name="jws_webresult_webresult1_server" basedir="." default="usage">
    <property name="app.name" value="WSWebResultWebServiceApp"/>
    <property name="service.names" value="webResultWebServiceService"/>
    <property name="sei.names" value="webResultWebService"/>
    <property name="port.name" value="webResultWebServicePort"/>
    <property name="wsdl.filename" value="WebResultWebServiceService.wsdl"/>
    <property name="impl.class" value="com.sun.ts.tests.jws.webresult.webresult1.server.webResultWebService"/>

    <!-- Import common classes properties and specific packaging file -->
    <import file="../../../../../../../../../src/com/sun/ts/tests/jws/common/xml/common.xml"/>

    <property name="wsdl.file" value="${src.dir}/${pkg.dir}/${wsdl.filename}"/>

    <property name="impl.classes" value="${pkg.dir}/webResultWebService.class"/>
    <property name="common.classes" value="${jws.common.classes}"/>
    <property name="other.classes" value="com/sun/ts/tests/jws/utils/**/*.class"/>

    <target name="-postcompile">
        <antcall  target="do_java2wsdl_generate_server"/>
    </target>

    <target name="package" depends="j2w_webservice_import_package"/>
    <target name="-preclean">
        <remove_wsdl_xsd/>
    </target>
</project>
